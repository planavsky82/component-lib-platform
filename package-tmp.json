{
  "name": "@planit/design-system",
  "version": "0.0.101",
  "description": "Design System Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/design-system/design-system.esm.js",
  "scripts": {
    "deploy:patch": "./scripts/deploy.sh patch",
    "deploy:minor": "./scripts/deploy.sh minor",
    "deploy:major": "./scripts/deploy.sh major",
    "build": "stencil build --docs",
    "start": "run-p build-stencil:watch storybook",
    "start:dev": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006 -s dist",
    "build-storybook": "build-storybook",
    "build-stencil:watch": "stencil build --docs-readme --watch --serve"
  },
  "dependencies": {
    "@stencil/core": "^2.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@stencil/postcss": "^2.1.0",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/html": "^6.4.9",
    "@types/jest": "^26.0.24",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "cssnano": "^5.0.14",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-replace": "^1.1.3",
    "puppeteer": "^10.0.0",
    "tailwindcss": "^3.0.7"
  },
  "author": "Anthony Planavsky",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bin": "bin/install.sh",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planavsky82/design-system.git"
  },
  "bugs": {
    "url": "https://github.com/planavsky82/design-system/issues"
  },
  "homepage": "https://github.com/planavsky82/design-system#readme"
}
